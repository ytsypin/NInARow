<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ChatServlet</servlet-name>
        <servlet-class>ninaRow.servlets.ChatServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>ninaRow.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsernameServlet</servlet-name>
        <servlet-class>ninaRow.servlets.UsernameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsersListServlet</servlet-name>
        <servlet-class>ninaRow.servlets.UsersListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>ninaRow.servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadGameServlet</servlet-name>
        <servlet-class>ninaRow.servlets.LoadGameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GameListServlet</servlet-name>
        <servlet-class>ninaRow.servlets.GameListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JoinGameServlet</servlet-name>
        <servlet-class>ninaRow.servlets.JoinGameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BasicInfoServlet</servlet-name>
        <servlet-class>ninaRow.servlets.gameServlets.BasicInfoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CurrentStatusServlet</servlet-name>
        <servlet-class>ninaRow.servlets.gameServlets.CurrentStatusServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsernameServlet</servlet-name>
        <url-pattern>/NinaRow/username</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChatServlet</servlet-name>
        <url-pattern>/NinaRow/chat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/NinaRow/pages/signup/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsersListServlet</servlet-name>
        <url-pattern>/NinaRow/userlist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadGameServlet</servlet-name>
        <url-pattern>/NinaRow/gameUpload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GameListServlet</servlet-name>
        <url-pattern>/NinaRow/gameList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/NinaRow/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JoinGameServlet</servlet-name>
        <url-pattern>/NinaRow/joinGame</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BasicInfoServlet</servlet-name>
        <url-pattern>/NinaRow/game/BasicInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CurrentStatusServlet</servlet-name>
        <url-pattern>/NinaRow/game/CurrentStatus</url-pattern>
    </servlet-mapping>
</web-app>
